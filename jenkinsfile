pipeline {
    environment {
        VERSION = "0.8-SNAPSHOT"
    }
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'mvn clean compile'
                
            }
        }
         stage('test') {
            steps {
                sh 'mvn test'
               
            }
        }
         stage('build') {
            steps {
                
                sh 'mvn package'
            }
        }
        
        stage('Create docker images') {
            steps {
               
                     sh './mvnw spring-boot:build-image'
                                    }
            
        }
    }

}
