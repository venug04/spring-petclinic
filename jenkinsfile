pipeline {
    environment {
        VERSION = "0.8-SNAPSHOT"
    }
    agent any
    stages {
        stage('Build') {
            steps {
                sh './mvnw clean compile'
                sh './mvnw test'
                sh './mvnw package'
            }
        }
        
        stage('Create docker images') {
            steps {
               
                     sh './mvnw spring-boot:build-image'
                                    }
            
        }
    }

}
