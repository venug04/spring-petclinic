pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh './mvnw clean compile'
                
            }
        }
         stage('test') {
            steps {
                sh './mvnw test'
               
            }
        }
         stage('build') {
            steps {
                
                sh './mvnw package -Dmaven.test.skip=true'
            }
        }
        
        stage('Create docker images') {
            steps {
               
                     sh './mvnw spring-boot:build-image'
                                    }
            
        }
    }

}
